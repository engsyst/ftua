<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.6.1//EN" "file:///D:\Apps\eclipse-gwt-kepler\gwt-2.6.1\gwt-module.dtd">
<module rename-to='error'>
	<inherits name="com.google.gwt.user.User" />
	<inherits name='com.google.gwt.user.theme.clean.Clean' />
	<entry-point class='ua.nure.ostpc.malibu.shedule.client.ErrorEntryPoint' />

	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />
	<source path='rpc' />

	<generate-with
		class="ua.nure.ostpc.malibu.shedule.rpc.proxy.CustomRpcRemoteProxyGenerator">
		<when-type-assignable class="com.google.gwt.user.client.rpc.RemoteService" />
	</generate-with>

	<!-- allow Super Dev Mode -->
	<add-linker name="xsiframe" />
</module>